<h1>Create Your Museum Visit</h1>


<%= form_for(@museum) do |f| %>
<div>
<%= f.label :name %>
<%= f.text_field :name %>
</div>
<br>
<div>
<%= f.label :country %>
<%= f.text_field :country %>
</div>
<br>
<div>
<%= f.label :city %>
<%= f.text_field :city %>
</div>
<br>

<%= f.submit "Visit"%>

<%end%>

