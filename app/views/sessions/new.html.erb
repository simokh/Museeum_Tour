<h1 class="title"> Museum Login</h1>

<%= form_for(:user) do |f| %>

<div>
    <%= f.label :user_name %>
    <%= f.text_field :user_name %>
</div>

<div>
    <%= f.label :password %>
    <%= f.password_field :password %>
</div>
 
    <%= f.submit "Login" %>

<div> 
    <%= flash.alert if flash.alert %>
</div>




<% end %>